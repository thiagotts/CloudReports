<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="cloudreports.models.NetworkMapEntry" table="NetworkMap">
    <id column="Id" name="id" type="long">
      <generator class="native"/>
    </id>
    <property column="Source" name="source" type="string"/>
    <property column="Destination" name="destination" type="string"/>
    <property column="Bandwidth" name="bandwidth" type="double"/>
    <property column="Latency" name="latency" type="double"/>
  </class>
</hibernate-mapping>
